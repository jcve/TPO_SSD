			left join [Aurorita].[dbo].[Dim_Region] Auroregion on Auroregion.Nombre like isNull(Region, 'OTROS')
			) tabla
		left join [Aurorita].[dbo].[Dim_Pais] auroPais on auropais.Nombre = tabla.pais
		) paises on paises.pais like a.pais</property>
