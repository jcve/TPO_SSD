                <property
                  dataType="System.String"
                  description="Comando SQL para ejecutar."
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT DISTINCT * FROM (

select distinct Year(isNull(fechanacimiento,'01/01/1900')) anio, auroAnio.IdAnio, month(isNull(fechanacimiento,'01/01/1900')) mes,
(select trimestre from getTrimestre(isNull(fechanacimiento,'01/01/1900'))) trimestre,  auroEstacion.IdEstacion, (select nombre from getnombreMes(isNull(fechanacimiento,'01/01/1900'))) nombre
from empleados
inner join [Aurorita].[dbo].[Dim_Anio] auroAnio on auroAnio.Anio = Year(isNull(fechanacimiento,'01/01/1900'))
inner join [Aurorita].[dbo].[Dim_Estacion] auroEstacion on auroEstacion.Nombre = (select estacion from getEstacion(isNull(fechanacimiento,'01/01/1900')))

union 

select distinct Year(isNull(FechaContratacion,'01/01/1900')) anio,auroAnio.IdAnio, month(isNull(FechaContratacion,'01/01/1900')) mes,
(select trimestre from getTrimestre(isNull(FechaContratacion,'01/01/1900'))) trimestre,  auroEstacion.IdEstacion, (select nombre from getNombreMes(isNull(FechaContratacion,'01/01/1900'))) nombre
from [Aurorita_Access].[dbo].[Empleados]
inner join [Aurorita].[dbo].[Dim_Anio] auroAnio on auroAnio.Anio = Year(isNull(FechaContratacion,'01/01/1900'))
