                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select resultado.Id, resultado.IdCliente, resultado.IdEmpleado, resultado.IdFormaEnvio, resultado.IdFechaPedido, resultado.IdFechaEntrega, ISNULL(resultado.IdFechaEnvio, resultado.IdFechaEntrega) as IdFechaEnvio, resultado.ValorTotal from (
  select
  p.Id, 
  dimp.Id as IdCliente,
  dimp2.Id as IdEmpleado,
  dimpce.Id as IdFormaEnvio,
  (
    select top 1 dimf.IdFecha
	from Aurorita.dbo.Dim_Fecha as dimf
  left join Aurorita.dbo.Dim_Dia dimd on dimd.Nombre = DATENAME(WEEKDAY, FechaPedido) and dimf.IdDia = dimd.IdDia 
  left join Aurorita.dbo.Dim_Mes dimm on dimm.Mes = Month(FechaPedido) and dimf.IdMes = dimm.IdMes
  left join Aurorita.dbo.Dim_Anio dima on dima.Anio = YEAR(FechaPedido) and dimm.IdAnio = dima.IdAnio
  where dimf.Dia = DAY(FechaPedido) AND dimm.Mes = Month(FechaPedido) AND dima.Anio = YEAR(FechaPedido)
  ) as IdFechaPedido,
    (
    select top 1 dimf.IdFecha
	from Aurorita.dbo.Dim_Fecha as dimf
  left join Aurorita.dbo.Dim_Dia dimd on dimd.Nombre = DATENAME(WEEKDAY, FechaEntrega) and dimf.IdDia = dimd.IdDia 
  left join Aurorita.dbo.Dim_Mes dimm on dimm.Mes = Month(FechaEntrega) and dimf.IdMes = dimm.IdMes
  left join Aurorita.dbo.Dim_Anio dima on dima.Anio = YEAR(FechaEntrega) and dimm.IdAnio = dima.IdAnio
  where dimf.Dia = DAY(FechaEntrega) AND dimm.Mes = Month(FechaEntrega) AND dima.Anio = YEAR(FechaEntrega)
  ) as IdFechaEntrega,
    (
    select top 1 dimf.IdFecha
	from Aurorita.dbo.Dim_Fecha as dimf
  left join Aurorita.dbo.Dim_Dia dimd on dimd.Nombre = DATENAME(WEEKDAY, FechaEnvio) and dimf.IdDia = dimd.IdDia 
  left join Aurorita.dbo.Dim_Mes dimm on dimm.Mes = Month(FechaEnvio) and dimf.IdMes = dimm.IdMes
  left join Aurorita.dbo.Dim_Anio dima on dima.Anio = YEAR(FechaEnvio) and dimm.IdAnio = dima.IdAnio
  where dimf.Dia = DAY(FechaEnvio) AND dimm.Mes = Month(FechaEnvio) AND dima.Anio = YEAR(FechaEnvio)
  ) as IdFechaEnvio
  ,
  /*(CAST(ISNULL(replace(p.Cargo, ',', '.'), 0) as decimal(18,2))) as ValorTotal*/
  cast(ISNULL(((
	  select SUM(IIF(dp.Descuento&gt;0, ((CAST(ISNULL(replace(dp.Precio, ',', '.'), 0) as decimal(18,2)))-((CAST(ISNULL(replace(dp.Precio, ',', '.'), 0) as decimal(18,2)))*(IIF(dp.Descuento=0, 1, dp.Descuento)))), (CAST(ISNULL(replace(dp.Precio, ',', '.'), 0) as decimal(18,2))))) from [Aurorita_Access].[dbo].DetallesPedidos dp
	  where dp.Id=p.Id
	  group by dp.Id
  )), CAST(ISNULL(replace(p.Cargo, ',', '.'), 0) as decimal(18,2))) as decimal(18,2)) as ValorTotal
  FROM [Aurorita_Access].[dbo].[Pedidos] p
  left join Aurorita.dbo.Dim_Persona dimp on dimp.Id = p.Cliente and dimp.Tipo = 2
  left join Empleados emp on emp.Id = p.Empleado
  left join Aurorita.dbo.Dim_Persona dimp2 on dimp2.Id = (emp.Apellido+' '+emp.Nombre) and dimp2.Tipo = 1
  left join CompaniaEnvios ce on ce.Id = p.FormaEnvio
  left join Aurorita.dbo.Dim_CompaniaEnvio dimpce on dimpce.Nombre = ce.Nombre
 ) resultado</property>
