                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select ciudad, a.pais , regionID, paisId from (
	select PaisDestinatario pais,ciudadDestinatario ciudad, Auroregion.Id regionID from pedidos
	left join [Aurorita].[dbo].[Dim_Region] Auroregion on Auroregion.Nombre like isNull(RegionDestinatario, 'OTROS')

